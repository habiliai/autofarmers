{
  "name": "autofarmers",
  "version": "0.1.0",
  "repository": "https://github.com/jcooky/autofarmers",
  "author": "HabiliAI Team",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:app": "prettier --write ./app",
    "format:components": "prettier --write ./components",
    "format:lib": "prettier --write ./lib",
    "format:data": "prettier --write ./data",
    "format": "run-s format:*",
    "generate:proto": "cd ./proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=../node_modules/.bin/protoc-gen-ts --proto_path=. --js_out=import_style=commonjs,binary:. --grpc_out=grpc_js:. --ts_out=service=grpc-node,mode=grpc-js:. *.proto",
    "setup": "docker compose up -d --build",
    "teardown": "docker compose down"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-query": "^5.71.5",
    "@tanstack/react-table": "^8.21.2",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "google-protobuf": "^3.21.4",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.487.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.71.5",
    "@types/google-protobuf": "^3.15.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "grpc-tools": "^1.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5"
  }
}
